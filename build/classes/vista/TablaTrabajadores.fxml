<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPanePrincipal" prefHeight="720.0" prefWidth="1260.0" style="-fx-background-radius: 20; -fx-background-color: linear-gradient(to bottom, #b2f0c2, #4caf50);" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.TablaTrabajadoresController">
   <children>
      <Pane prefHeight="720.0" prefWidth="1260.0" style="-fx-background-radius: 20; -fx-background-color: linear-gradient(to bottom, #b2f0c2, #4caf50);">
         <children>
            <TableView fx:id="tablaTrabajadores" layoutX="230.0" layoutY="59.0" prefHeight="604.0" prefWidth="800.0">
              <columns>
                <TableColumn fx:id="columnaNombre" prefWidth="372.0" text="Nombre" />
                <TableColumn fx:id="columnaHoras" prefWidth="231.0" text="Horas semanales" />
                  <TableColumn fx:id="columnaPropina" prefWidth="196.0" text="Propina a recibir" />
              </columns>
            </TableView>
            <Button fx:id="botonVolver" layoutX="14.0" layoutY="681.0" mnemonicParsing="false" onAction="#botonVolver" prefHeight="25.0" prefWidth="85.0" style="-fx-background-color: linear-gradient(to bottom, #ffeb3b, #ff9800);" text="Volver">
               <font>
                  <Font name="Century Gothic Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="botonGuardar" layoutX="1161.0" layoutY="681.0" mnemonicParsing="false" onAction="#botonGuardar" prefHeight="25.0" prefWidth="85.0" style="-fx-background-color: linear-gradient(to bottom, #ffeb3b, #ff9800);" text="Guardar">
               <font>
                  <Font name="Century Gothic Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
